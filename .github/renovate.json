{
  "extends": [
    "config:base",
    ":disableDependencyDashboard",
    ":automergeMinor"
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "packagePatterns": [
        "^github.com/aws/aws-sdk-go-v2$",
        "^github.com/aws/aws-sdk-go-v2/"
      ],
      "automerge": true,
      "groupName": "all aws-sdk-go-v2 dependencies",
      "groupSlug": "all-aws-sdk-go-v2"
    },
    {
      "matchSourceUrls": [
        "https://github.com/facebook/lexical"
      ]
    }
  ],
  "postUpdateOptions": ["gomodTidy", "pnpmDedupe"]
}
